    <text class="cell_cn">消息</text>
    <view class="cell_ft">
    <i class="iconfont icon-go"></i>
    </view>
  </view>
  <view class="cell" bindtap='gohome'>
    <text class="cell_cn">我的花房</text>
    <view class="cell_ft"><i class="iconfont icon-go"></i></view>
  </view>
  <view class="cell" bindtap='goOrder'>
    <text class="cell_cn">我的订单</text>
    <view class="cell_ft"><i class="iconfont icon-go"></i></view>
  </view> -->
  <view class="cell" bindtap='goCollect'>
    <text class="cell_cn">我的关注</text>
    <view class="cell_ft"><i class="iconfont icon-go"></i></view>
  </view>
  <view class="cell" bindtap='goAddress'>
    <text class="cell_cn">地址管理</text>
    <view class="cell_ft">
      <i class="iconfont icon-go"></i>
    </view>
  </view>
  <navigator hover-class="none" url="../../doc/about/about"><view class="cell">
    <text class="cell_cn">意见反馈</text>
    <view class="cell_ft"><i class="iconfont icon-go"></i></view>
  </view></navigator>  
  <navigator hover-class="none" url="../../doc/about/about"><view class="cell">
    <text class="cell_cn">关于花兮</text>
    <view class="cell_ft"><i class="iconfont icon-go"></i></view>
  </view></navigator>  
  
