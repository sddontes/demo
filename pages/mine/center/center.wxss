/* pages/mine/center/center.wxss */
.container{
  padding-top: 38rpx;
}
.header_icon{
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  border: 1px solid #f5f5f5;
  position: relative;
  margin-left: 40rpx;
}
.header_name{
  font-size: 32rpx;
  color: #1C1B21;
  margin-left: 30rpx;
  align-self: center;
  font-weight: 500;
}
.header_button {
  align-self: center;
  background-color: #eee;
  margin-left: 140rpx;
  padding: 20rpx;
  border-radius: 15rpx;
  box-shadow: 5rpx 15rpx 15rpx gray;
}
.v_icon{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  bottom: -8rpx;
  right: 0;
}
.center_nav{
  padding: 32rpx 104rpx 50rpx;
  text-align: center;
  color: #1C1B21;
}
.nav_item{
  font-size: 26rpx;
  margin-top: 12rpx;
}
.blank_bar{
  width: 100%;
  height: 16rpx;
  background: #f9f9f9;
}
.new_num{
  display: inline-block;
  background: #FF5142;
  color: #fff;
  line-height: 36rpx;
  padding: 0 16rpx;
  font-size: 30rpx;
  border-radius: 100rpx;
  margin-right: 3rpx;
}
.order_items {
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 40rpx;
  padding: 10rpx 40rpx;
  /* border-bottom: 1rpx solid #eee; */
}
.order_all {
  display: block;
  float: right;
}
.henader_header {
  margin-bottom: 60rpx;
}
.order_item_icon {
  margin: 0rpx auto;
  width: 60rpx;
  height: 60rpx;
}